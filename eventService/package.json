  "scripts": {
    "start": "tsc && nodemon build/index.js env=test",
    "buildmac": "rm -rf dist && mkdir dist && webpack --mode development && cp package.json dist && cp -r config dist && cp -r email dist",
    "build-prod-mac": "rm -rf dist && mkdir dist && webpack --mode production && cp package.json dist && cp -r config dist && cp -r email dist",
    "buildwindow": "rmdir /s /q dist && webpack --mode development && copy package.json .\\dist\\ && xcopy /y config .\\dist\\config\\",
    "build-prod-window": "rmdir /s /q dist && webpack --mode production && copy package.json .\\dist\\ && xcopy /y config .\\dist\\config\\",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
